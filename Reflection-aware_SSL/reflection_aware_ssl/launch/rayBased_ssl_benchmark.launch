<launch>
	<arg name="urdf_file" default="$(find xacro)/xacro.py '$(find kobuki_description)/urdf/kobuki_standalone.urdf.xacro'" />
	<param name="robot_description" command="$(arg urdf_file)" />
		
	<node pkg="tf" type="static_transform_publisher" name="kobukiModel_link" args="0.0 0.0 0.0 0 0 0 KobukiV1 base_footprint 100" />

	<!--<node pkg="nodelet" type="nodelet" name="mobile_base_nodelet_manager" args="manager"/>
	<node pkg="nodelet" type="nodelet" name="mobile_base" args="load kobuki_softnode/SoftKobukiNodelet mobile_base_nodelet_manager">
		<rosparam file="$(find kobuki_softnode)/param/base_r1.yaml" command="load"/>
		<remap from="mobile_base/odom" to="odom"/>
		<remap from="mobile_base/joint_states" to="joint_states"/>
	</node>-->


	<node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher" output="screen">
    		<param name="publish_frequency" type="double" value="100" />
  	</node>
  	<node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher">
    		<param name="use_gui" value="false"/>
  	</node>


	<node pkg="reflection_aware_ssl" type="reflection_aware_ssl_benchmark_node" name="reflection_aware_ssl_benchmark_node" output="screen">

	</node>
</launch>
